# Ejemplo de archivo .env - Copia este archivo como .env y completa con tus datos

# Google OAuth2 Credentials
# Obt√©n estos valores de Google Cloud Console
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:8001/api/auth/callback

# Firebase Configuration
# Ruta al archivo de credenciales de Firebase Service Account
FIREBASE_SERVICE_ACCOUNT_PATH=./config/firebase-service-account.json

# Server Configuration
API_HOST=localhost
API_PORT=8001
DEBUG=True

# Google Scopes (no cambiar a menos que sepas lo que haces)
GOOGLE_SCOPES=https://www.googleapis.com/auth/calendar
